@model IEnumerable<PortoWork.Models.Project>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.City.city_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Service.serv_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User1.user_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.pro_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.pro_text)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.img_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cost)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.City.city_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Service.serv_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User1.user_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.pro_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.pro_text)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.img_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cost)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.pro_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.pro_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.pro_id })
        </td>
    </tr>
}

</table>
